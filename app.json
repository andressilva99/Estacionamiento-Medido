{
  "expo": {
    "name": "Estacionamiento Medido San Francisco",
    "slug": "estacionamiento-medido-frontend",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sanfrancisco.gov.ar.EstacionamientoMedidoSanFranciso"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "package": "com.sanfrancisco.gov.ar.EstacionamientoMedidoSanFranciso"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-notifications"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5afc0467-ef9c-4cd2-809e-9a7d58ade897"
      }
    }
  }
}
